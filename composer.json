{
    "require": {
        "ext-zip": "*",
        "elasticsearch/elasticsearch": "^7.15",
        "monolog/monolog": "^2.2"
    },
    "scripts": {
        "es-run": [
            "docker pull docker.elastic.co/elasticsearch/elasticsearch:7.15.1",
            "docker run -p 9200:9200 -p 9300:9300 -e \"discovery.type=single-node\" --name elasticsearch-php-examples -d docker.elastic.co/elasticsearch/elasticsearch:7.15.1"
        ],
        "es-stop": [
            "docker stop elasticsearch-php-examples"
        ]
    }
}
